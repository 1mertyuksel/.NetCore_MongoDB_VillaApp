@{
    Layout = null;
}

<!DOCTYPE html>

<html lang="en">

@await Component.InvokeAsync("_AdminHead")
<body>

    
    @await Component.InvokeAsync("_AdminPreloader")

   
  
    <div id="main-wrapper">

        <!--LogoSection-->
        @await Component.InvokeAsync("_AdminLogo")

        @await Component.InvokeAsync("_AdminNavbar")

        <!--**********************************
            Sidebar start
        ***********************************-->
        @await Component.InvokeAsync("_AdminSideBar")
        <!--**********************************
            Sidebar end
        ***********************************-->
        <!--**********************************
            Content body start
        ***********************************-->
        <div class="content-body">
            <div class="container-fluid">
                <div class="row page-titles mx-0">
                    <div class="col-sm-6 p-md-0">
                        <div class="welcome-text">
                            <h4>@ViewData["Title"]</h4>
                        </div>
                    </div>
                   
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <div class="card">
                           
                            <div class="card-body">
                                @RenderBody()
                            </div>
                            
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--**********************************
            Content body end
        ***********************************-->
        <!--**********************************
            Footer start
        ***********************************-->
        @await Component.InvokeAsync("_AdminFooter")

        <!--**********************************
            Footer end
        ***********************************-->
        <!--**********************************
           Support ticket button start
        ***********************************-->
        <!--**********************************
           Support ticket button end
        ***********************************-->


    </div>
    <!--**********************************
        Main wrapper end
    ***********************************-->
    <!--**********************************
        Scripts

    ***********************************-->
    @await Component.InvokeAsync("_AdminScripts")

    <!-- Required vendors -->
    
    <!-- Circle progress -->

</body>

</html>